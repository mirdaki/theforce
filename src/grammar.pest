Program = _{ SOI ~ Main ~ EOI }

Main = { BeginMain ~ Expr ~ EndMain }

Expr = { Print ~ String }

String  = @{ "\"" ~ Inner ~ "\"" }
Inner   = @{ (!("\"" | "\\") ~ ANY)* ~ (Escape ~ Inner)? }
Escape  = @{ "\\" ~ ("\"" | "\\" | "/" | "b" | "f" | "n" | "r" | "t" ) }

WHITESPACE = _{ " " | "\t" }

BeginMain = _{ "Do it" }

EndMain = _{ "May The Force be with you" }

Print = _{ "The Sacred Texts!" }
